.panel.gmd
  border 0
  border-radius 2px
  box-shadow: 0 1px 3px 0 rgba(0,0,0,.2),0 1px 1px 0 rgba(0,0,0,.14),0 2px 1px -1px rgba(0,0,0,.12)
  padding: 24px 16px 16px
  
.panel.gmd .panel-heading
  font-size: 1.4em
  
.panel.gmd .panel-footer
  padding: 10px
  text-align: right
  background-color: #fff
  border-top: 0